root = true

[*]
indent_style = tab
tab_width = 4
end_of_line = lf
charset = utf-8
[*.{html, css, js}]
trim_trailing_whitespace = true
insert_final_newline = true

[*.{md}]
insert_final_newline = true
trim_trailing_whitespace = false

[{package.json, package-lock.json}]
indent_style = space
indent_size = 2